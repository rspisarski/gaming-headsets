            "Active Noise Cancellation",
            "Transparency Mode",
            "Ambient Sound Mode",
            "Wear Detection",
            "Auto Play/Pause"
          ]
        },
        {
          "title": "RGB Lighting",
          "name": "rgb_lighting",
          "type": "choice",
          "choices": [
            "Customizable RGB",
            "Static RGB",
            "Reactive Lighting",
            "No Lighting"
          ],
          "default": "No Lighting"
        },
        {
          "title": "Special Features",
          "name": "special_features",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Haptic Feedback",
            "Head Tracking",
            "Voice Assistant",
            "Multi-point Connection",
            "Low Latency Mode",
            "Game/Chat Mix",
            "Swappable Batteries",
            "Quick Charge"
          ]
        },
        {
          "title": "Has Software",
          "name": "has_software",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Software Name",
          "name": "software_name",
          "type": "string"
        },
        {
          "title": "Software Features",
          "name": "software_features",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Custom EQ",
            "EQ Presets",
            "Mic Enhancement",
            "Button Remapping",
            "Firmware Updates",
            "Cloud Sync"
          ]
        },
        {
          "title": "Certifications",
          "name": "certifications",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Hi-Res Audio",
            "Discord Certified",
            "TeamSpeak Certified",
            "Microsoft Teams Certified",
            "Works with PlayStation",
            "Designed for Xbox",
            "Made for iPhone",
            "Nintendo Licensed"
          ]
        },
        {
          "title": "Warranty Years",
          "name": "warranty_years",
          "type": "number",
          "default": 1
        },
        {
          "title": "Water Resistant",
          "name": "water_resistant",
          "type": "boolean",
          "default": false
        },
        {
          "title": "IP Rating",
          "name": "ip_rating",
          "type": "string"
        },
        {
          "title": "Included Accessories",
          "name": "included_accessories",
          "type": "choice",
          "multiple": true,
          "choices": [
            "USB Dongle",
            "USB Cable",
            "Carrying Case",
            "Extra Ear Pads"
          ]
        },
        {
          "title": "Cable Length (feet)",
          "name": "cable_length",
          "type": "number"
        },
        {
          "title": "Cable Features",
          "name": "cable_features",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Detachable Cable",
            "Braided Cable",
            "Inline Controls",
            "Y-splitter"
          ]
        },
        {
          "title": "Amazon US URL",
          "name": "amazon_us",
          "type": "string"
        },
        {
          "title": "Amazon CA URL",
          "name": "amazon_ca",
          "type": "string"
        },
        {
          "title": "Amazon UK URL",
          "name": "amazon_uk",
          "type": "string"
        },
        {
          "title": "Best Buy URL",
          "name": "bestbuy",
          "type": "string"
        },
        {
          "title": "Newegg URL",
          "name": "newegg",
          "type": "string"
        },
        {
          "title": "Manufacturer URL",
          "name": "manufacturer_url",
          "type": "string",
          "required": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "required": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Best For",
          "name": "best_for",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Console Gaming",
            "Multi-platform",
            "Competitive Gaming",
            "Competitive FPS",
            "Streaming"
          ]
        }
      ]
    },
    {
      "name": "blog",
      "previewPath": "/blog/{{slug}}",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Excerpt",
          "name": "excerpt",
          "type": "string",
          "required": true
        },
        {
          "title": "Category",
          "name": "category",
          "type": "string",
          "required": true
        },
        {
          "title": "Image URL",
          "name": "image",
          "type": "string",
          "required": true
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "Published Date",
          "name": "published",
          "type": "datetime",
          "required": true
        },
        {
          "title": "Updated Date",
          "name": "updated",
          "type": "datetime"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ]
}