{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "Headsets",
      "path": "[[workspace]]/src/content/headsets"
    },
    {
      "title": "Blog",
      "path": "[[workspace]]/src/content/blog"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "headsets",
      "previewPath": "/headsets/{{slug}}",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Name",
          "name": "name",
          "type": "string",
          "required": true
        },
        {
          "title": "Brand",
          "name": "brand",
          "type": "string",
          "required": true
        },
        {
          "title": "Model",
          "name": "model",
          "type": "string"
        },
        {
          "title": "Price",
          "name": "price",
          "type": "number",
          "required": true
        },
        {
          "title": "Image URL",
          "name": "image",
          "type": "string",
          "required": true
        },
        {
          "title": "Thumbnail Contain",
          "name": "image_thumbnail_contain",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Release Date",
          "name": "release_date",
          "type": "datetime"
        },
        {
          "title": "Amazon Product ID",
          "name": "amazon_product_id",
          "type": "string"
        },
        {
          "title": "Discontinued",
          "name": "discontinued",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Connection Types",
          "name": "connection_types",
          "type": "choice",
          "multiple": true,
          "choices": [
            "3.5mm",
            "USB-A",
            "USB-C",
            "2.4GHz Wireless",
            "Bluetooth"
          ]
        },
        {
          "title": "Battery Life (hours)",
          "name": "battery_life",
          "type": "number"
        },
        {
          "title": "Charging Type",
          "name": "charging_type",
          "type": "choice",
          "choices": [
            "USB-C",
            "USB-A",
            "Micro-USB",
            "Wireless Charging"
          ]
        },
        {
          "title": "Wireless Range (feet)",
          "name": "wireless_range",
          "type": "number"
        },
        {
          "title": "Wireless Latency (ms)",
          "name": "wireless_latency",
          "type": "number"
        },
        {
          "title": "Platforms",
          "name": "platforms",
          "type": "choice",
          "multiple": true,
          "choices": [
            "PC",
            "PlayStation 5",
            "PlayStation 4",
            "Xbox Series X|S",
            "Xbox One",
            "Nintendo Switch",
            "Mobile",
            "Steam Deck",
            "VR Headsets"
          ]
        },
        {
          "title": "Xbox Wireless Native",
          "name": "xbox_wireless_native",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Driver Size (mm)",
          "name": "driver_size",
          "type": "number"
        },
        {
          "title": "Driver Type",
          "name": "driver_type",
          "type": "choice",
          "choices": [
            "Dynamic",
            "Planar Magnetic",
            "Electrostatic",
            "Hybrid",
            "Graphene"
          ]
        },
        {
          "title": "Frequency Response",
          "name": "frequency_response",
          "type": "string"
        },
        {
          "title": "Impedance (ohms)",
          "name": "impedance",
          "type": "number"
        },
        {
          "title": "Sensitivity (dB)",
          "name": "sensitivity",
          "type": "number"
        },
        {
          "title": "Surround Sound",
          "name": "surround_sound",
          "type": "choice",
          "choices": [
            "Stereo",
            "Virtual 5.1",
            "Virtual 7.1",
            "True 7.1",
            "Dolby Atmos",
            "DTS:X",
            "Windows Sonic",
            "THX Spatial Audio",
            "Tempest 3D Audio",
            "Proprietary Spatial"
          ]
        },
        {
          "title": "Headphone Type",
          "name": "headphone_type",
          "type": "choice",
          "choices": [
            "Closed-back",
            "Open-back",
            "Semi-open"
          ]
        },
        {
          "title": "Microphone Type",
          "name": "microphone_type",
          "type": "choice",
          "choices": [
            "Boom Mic",
            "Retractable Mic",
            "Detachable Mic",
            "Inline Mic",
            "Dual Microphone Array",
            "No Microphone"
          ]
        },
        {
          "title": "Mic Detachable",
          "name": "mic_detachable",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Mic Frequency Response",
          "name": "mic_frequency_response",
          "type": "string"
        },
        {
          "title": "Mic Features",
          "name": "mic_features",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Noise Cancellation",
            "AI Noise Cancellation",
            "Pop Filter",
            "Sidetone",
            "Mute Button",
            "Auto-mute",
            "LED Mute Indicator",
            "Broadcast Quality",
            "Beamforming"
          ]
        },
        {
          "title": "Mic Monitoring",
          "name": "mic_monitoring",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Ear Cup Design",
          "name": "ear_cup_design",
          "type": "choice",
          "choices": [
            "Over-ear",
            "On-ear"
          ]
        },
        {
          "title": "Cushion Materials",
          "name": "cushion_materials",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Memory Foam",
            "Gel-infused",
            "Velour",
            "Leatherette",
            "Genuine Leather",
            "Microfiber",
            "Breathable Fabric"
          ]
        },
        {
          "title": "Build Material",
          "name": "build_material",
          "type": "choice",
          "choices": [
            "Plastic",
            "Steel",
            "Aluminum",
            "PVD-coated Steel",
            "Carbon Fiber"
          ]
        },
        {
          "title": "Weight (grams)",
          "name": "weight",
          "type": "number"
        },
        {
          "title": "Comfort Features",
          "name": "comfort_features",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Adjustable Headband",
            "Ski-goggle Suspension",
            "Replaceable Ear Pads",
            "Glasses-friendly",
            "Foldable",
            "Rotating Ear Cups",
            "Lightweight",
            "Reinforced Chassis",
            "Durable Frame"
          ]
        },
        {
          "title": "Onboard Controls",
          "name": "onboard_controls",
          "type": "choice",
          "multiple": true,
          "choices": [
            "Volume Wheel",
            "Mute Button",
            "Power Button",
            "Bluetooth Button",
            "Game/Chat Mix",
            "PLAYSYNC Button"
          ]
